{
  "name": "react-aomini",
  "version": "1.1.6",
  "description": "react mini framework aomini",
  "main": "./libs/index.js",
  "directories": {
    "example": "examples"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
